name: dockercompose6951411703743368746
services:
  mssql:
    container_name: mssql
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Chien2003x.
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: volume
        source: sql_volume
        target: /var/opt/mssql
        volume: {}
  web1:
    build:
      context: D:\source\repos\Web1
      dockerfile: Web1/Dockerfile
    depends_on:
      mssql:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_URLS: http://+:80;https://+:443
      ConnectionStrings__TinTucDB: Server=mssql;Database=Study;User Id=sa;Password=Chien2003x.;TrustServerCertificate=True
    image: web1
    networks:
      default: null
    ports:
      - mode: ingress
        target: 443
        published: "443"
        protocol: tcp
      - mode: ingress
        target: 80
        published: "8080"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\PC\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\PC\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose6951411703743368746_default
volumes:
  sql_volume:
    name: dockercompose6951411703743368746_sql_volume