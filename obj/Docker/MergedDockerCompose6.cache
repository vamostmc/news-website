name: dockercompose6951411703743368746
services:
  web1:
    build:
      context: D:\source\repos\Web1
      dockerfile: Web1/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionStrings__TinTucDB: Server=host.docker.internal;Database=Study;User Id=sa;Password=Chien2003x.;TrustServerCertificate=True
